package marupadi.ecommerce;

public class Products {
	
	String productName;		//product1, product2, product3
	float productPrice;		//Double Value
	int productQuantity;	//10,20,30...   
	String productSoldBy;	// seller1, seller2, seller3

	public String getProductName() {
		return productName;
	}

	public void setProductName(String productName) {
		this.productName = productName;
	}

	public float getProductPrice() {
		return productPrice;
	}
	
	public void setProductPrice(float productPrice) {
		this.productPrice = productPrice;
	}
	

	
	public int getProductQuantity() {
		return productQuantity;
	}
	
	public void setProductQuantity(int productQuantity) {
		this.productQuantity = productQuantity;
	}



	public String getProductSoldBy() {
		return productSoldBy;
	}

	public void setProductSoldBy(String productSoldBy) { 
		this.productSoldBy = productSoldBy;
	}
	



}
