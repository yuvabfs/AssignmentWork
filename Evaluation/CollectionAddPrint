package com.test.java;


import java.util.*;

public class CollectionAddPrint {

	public static void main(String[] args) {
		
		//Given Input Array
		int a[]= {1,1,5,5,10,20,15,10};
		
		
		//ArrayList Created with the Given array elements
		List<Integer> list1=new ArrayList<>();
		for(int i=0;i<8;i++)
		{
			list1.add(a[i]);
		}
		System.out.println("ArrayList of int[a] elements\n"+list1);
		
		
		//LinkedHashSet Created by passing the List entirely
		Set<Integer> linkedHashSet=new LinkedHashSet<>(list1);
		System.out.println("LinkedHashSet of int[a] elements\n"+linkedHashSet);
		
		
		//HashSet Created by passing list entirely
		Set<Integer> hashSet=new HashSet<>(list1);
		System.out.println("HashSet of int[a] elements\n"+hashSet);

	}

}
